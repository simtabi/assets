{
    "name": "simtabi/assets",
    "description": "Laravel assets management",
    "license": "MIT",
    "keywords": [
        "simtabi",
        "simtabi cms",
        "simtabi platform",
        "simtabi assets"
    ],
    "homepage": "https://simtabi.com",
    "authors": [
        {
            "name": "Imani Manyara",
            "email": "imani@simtabi.com"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0",
        "laravel/framework": "^5.6|^6|^7|^8"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.0|^8.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Simtabi\\Assets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Simtabi\\Assets\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Simtabi\\Assets\\Providers\\AssetsServiceProvider"
            ],
            "aliases": {
                "Assets": "Simtabi\\Assets\\Facades\\AssetsFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
